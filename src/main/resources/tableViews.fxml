<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sample.mealsTablesController">

    <VBox spacing="10">
        <HBox id="HBox" />
    </VBox>

    <VBox spacing="10" />


    <VBox layoutX="770.0" layoutY="585.0" spacing="10">
        <HBox alignment="BOTTOM_RIGHT" spacing="10">
            <Button fx:id="okButton" prefHeight="25.0" prefWidth="60.0" text="OK" />
            <Button fx:id="cancelButton" prefHeight="25.0" prefWidth="60.0" text="Chiudi" />
        </HBox>
    </VBox>
    <TableView fx:id="tbDietItems" layoutX="33.0" layoutY="54.0" prefHeight="518.0" prefWidth="870">
        <columns>
            <TableColumn fx:id="id" text="ID" visible="false">
            </TableColumn>
            <TableColumn fx:id="unitFood" text="Alimento">
            </TableColumn>
            <TableColumn fx:id="totKcal" text="Tot Kcal" prefWidth="150">
            </TableColumn>
            <TableColumn fx:id="grCarboidrati" text="Gr. Carboidrati" prefWidth="150">
            </TableColumn>
            <TableColumn fx:id="grProtein" text="Gr. Proteine" prefWidth="150">
            </TableColumn>
            <TableColumn fx:id="grGrassi" text="Gr. Grassi" prefWidth="150">
            </TableColumn>
        </columns>



    </TableView>
    <TextField fx:id="filterField" layoutX="99.0" layoutY="18.0" prefWidth="200" />


    <Label layoutX="33.0" layoutY="22.0" text="Cerca: " />

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

</AnchorPane>

