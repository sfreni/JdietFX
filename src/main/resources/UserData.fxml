<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="275.0" prefWidth="305.0" fx:id="mainBorderPane" fx:controller="sample.ControllerUserData"
      xmlns:fx="http://javafx.com/fxml">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="250.0" prefWidth="305.0" VBox.vgrow="ALWAYS" >
      <children>
            <Label layoutX="14.0" layoutY="14.0" prefHeight="27.0" prefWidth="185.0" text="Dati Utente">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="68.0" text="Nome" />
            <TextField layoutX="95.0" layoutY="64.0" fx:id="nome"  />
            <Label layoutX="14.0" layoutY="105.0" text="Cognome" />
            <TextField layoutX="95.0" layoutY="101.0" fx:id="cognome" />
            <Label layoutX="14.0" layoutY="142.0" prefHeight="17.0" prefWidth="85.0" text="Altezza(CM): " />
            <TextField layoutX="95.0" layoutY="138.0" fx:id="altezza"  />
            <Label layoutX="14.0" layoutY="179.0" prefHeight="17.0" prefWidth="85.0" text="Peso(KG):" />
            <TextField layoutX="95.0" layoutY="175.0" fx:id="peso"  />

          <Label layoutX="14.0" layoutY="216.0" prefHeight="17.0" prefWidth="85.0" text="User: " />
          <TextField layoutX="95.0" layoutY="212.0" fx:id="userLogin"  />
          <Label layoutX="14.0" layoutY="253.0" prefHeight="17.0" prefWidth="85.0" text="Password:" />
          <TextField layoutX="95.0" layoutY="249.0" fx:id="password"  />




          <Label layoutX="14.0" layoutY="290.0" prefHeight="17.0"  text="Stampare Altezza(CM):" />
          <CheckBox layoutX="220.0" layoutY="290.0" mnemonicParsing="false" selected="true"  focusTraversable="false" fx:id="altezzaPrint"/>
          <Label layoutX="14.0" layoutY="327.0" prefHeight="17.0"  text="Stampare Peso(KG):" />
          <CheckBox layoutX="220.0" layoutY="327.0" mnemonicParsing="false" selected="true"  focusTraversable="false" fx:id="pesoPrint"/>
            <Button fx:id="okButton" layoutX="125.0" layoutY="364.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="58.0" text="Ok" />
            <Button fx:id="cancelButton" layoutX="190.0" layoutY="364.0" mnemonicParsing="false" text="Chiudi" />
      </children>
    </AnchorPane>
  </children>
</VBox>
